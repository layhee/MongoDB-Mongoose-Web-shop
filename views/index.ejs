<!DOCTYPE html>
<html>
<!-----------header calls---------->
<%- include('../partials/head.ejs')%>
<%- include('../partials/html_header.ejs')%>
<!----------end headers------------>
<div>
      <div id="store-container">
        <%items.forEach(item => { %>
          <a href="/store/<%=item._id%>">
            <h2><%=item.name%></h2>
            <img src="<%=item.img%>"></a><br>
            <%=item.description%>
            <form action="/store/<%=item._id%>?_method=DELETE" method="POST">
              <input type="submit" value="DELETE" />
            </form>
           <%})%>          
      </div>
  </body>
</html>